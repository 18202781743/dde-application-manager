[Unit]
Description=dde application manager service
RefuseManualStart=no
RefuseManualStop=no
#OnFailure=dde-session-shutdown.target
OnFailureJobMode=replace-irreversibly
CollectMode=inactive-or-failed
StartLimitIntervalSec=10s
StartLimitBurst=30

#PartOf=dde-session-daemon.target

[Service]
Type=simple
ExecStart=/usr/bin/deepin-application-manager
TimeoutStartSec=infinity
Slice=components.slice
Restart=on-failure
RestartSec=300ms


#[Install]
#WantedBy=dde-session-daemon.target
